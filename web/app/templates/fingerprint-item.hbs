<div class="fingerprint-item">
    <div class="pure-g-r">
        <div class="pure-u-2-3">
            <h3>{{unbound name}}</h3>
            <h4 class="secondary">{{unbound fingerprint}}</h4>
        </div>
        <div class="pure-u-1-3 align-right">
            <span>view details on</span>
            <a href="{{unbound view.atlasUrl}}">Atlas</a>
            <a href="{{unbound view.globeUrl}}">Globe</a>
            <a href="{{unbound view.blutmagieUrl}}">Blutmagie</a>
        </div>
    </div>
</div>